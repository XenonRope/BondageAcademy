{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@bondage-academy/bondage-academy-model": [
        "libs/bondage-academy-model/src/index.ts"
      ]
    },
    "plugins": [
      {
        "name": "ts-plugin-sort-import-suggestions"
      }
    ]
  },
  "exclude": ["node_modules", "tmp"]
}
